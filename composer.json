{
  "name": "matucana/vk-phone-parser",
  "description": "Parser phone numbers vk.com",
  "type": "project",
  "keywords": [
    "vk.com"
  ],
  "homepage": "https://github.com/matucana/vk-phone-parser",
  "license": "MIT",
  "support": {
    "email": "victor.dialogbox@gmail.com"
  },
  "authors": [
    {
      "name": "Vinogradov Victor",
      "email": "victor.dialogbox@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.4",
    "symfony/dotenv": "5.2.*",
    "vkcom/vk-php-sdk": "5.101.*",
    "usmanhalalit/pixie": "2.0.*",
    "brick/phonenumber": "0.2.*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "suggest": {
    "monolog/monolog": "Allows more advanced logging of the application flow"
  },
  "autoload": {
    "psr-4": {
      "Matucana\\VkPhoneParser\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Matucana\\VkPhoneParser\\Test\\": "tests/"
    }
  }
